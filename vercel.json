{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "area-iq.com" }],
      "destination": "https://www.area-iq.com/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        { "type": "host", "value": "www.area-iq.com" },
        { "type": "header", "key": "x-forwarded-proto", "value": "http" }
      ],
      "destination": "https://www.area-iq.com/:path*",
      "permanent": true
    },
    {
      "source": "/:path+/",
      "destination": "/:path+",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/ads.txt", "destination": "/ads.txt" },
    { "source": "/api/:path*", "destination": "/api/:path*" },
    { "source": "/:path*", "destination": "/index.html" }
  ]
}

